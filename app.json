{
  "expo": {
    "name": "destination",
    "slug": "destination-australia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pawan.destinationaustralia"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9d95eea1-282d-4826-9f11-58bdfb49b5d1"
      }
    },
    "owner": "pawan1awasthi",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "This app requires access to you gallery."
        }
      ]
    ]
  }
}
